    {"name":"puppet/nginx",                              "version_requirement":">= 0.4.0"},
    {"name":"puppet/nodejs",                             "version_requirement":">= 2.1.0"},
    {"name":"puppetlabs/vcsrepo",                        "version_requirement":">= 1.4.0"},
